<div id='goal'>
  <h1>Today my goal is</h1>
  <%= form_for @conn, page_path(@conn, :submit_goal), [name: :submit_goal, method: 'post'], fn f -> %>
    <%= text_input f, :goal_input %>
    <input id='goal_submit' class='button-primary' value='Hey! Oh! Lets Go!' type='submit' maxlength='200'>
  <% end %>
</div>
